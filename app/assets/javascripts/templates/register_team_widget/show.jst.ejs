<div class="group">
  <h3>{{= name }}</h3>

  <div>
  {{ if(Object.keys(players).length > 0) { }}
    <table class="table table-hover table-condensed">
      <thead>
        <tr>
          <th>Name</th>
          <th>Age</th>
          <th>Number</th>
          <th></th>
        </tr>
      </thead>

      <tbody>
        {{ for(var index in players) { }}
          {{ if(players.hasOwnProperty(index)) { }}
            <tr>
              <td>
                {{= players[index].name }}
              </td>
              <td>
                {{= players[index].age }}
              </td>
              <td>
                {{= players[index].number }}
              </td>
            </tr>
          {{ } }}
        {{ } }}
      </tbody>
    </table>
    {{ } else { }}
      <p>There are no players in this team</p>
    {{ } }}
  </div>
</div>